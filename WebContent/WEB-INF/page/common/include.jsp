<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>

<!--这个是IE8的专用标记，是用来指定Internet Explorer 8 浏览器模拟某个特定版本IE浏览器的渲染方式，以此来解决IE浏览器的兼容问题。 -->
<!-- <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible"> -->
<!--浏览器渲染及高速模式  为了让 IE 浏览器运行最新的渲染模式下  -->
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<!-- 在双核浏览器情况下,使用极速模式 webkit内核渲染-->
<meta name="renderer" content="webkit">
<!-- 以下三句搭配用清缓存-->
<meta http-equiv="pragma" content="no-cache">
<meta http-equiv="cache-control" content="no-cache">
<meta http-equiv="Expires" content="0">

<!-- import jQuery -->
<script type="text/javascript" src="${pageContext.request.contextPath}/WebRoot/plugin/jquery/jquery-2.1.4.min.js"></script>
<script type="text/javascript" src="${pageContext.request.contextPath}/WebRoot/plugin/jquery/jquery.md5.js" ></script>
<!-- import Bootstrap -->
<script type="text/javascript" src="${pageContext.request.contextPath}/WebRoot/plugin/bootstrap-3.3.6-dist/js/bootstrap.min.js"></script>
<!-- import Bootbox -->
<script type="text/javascript" src="${pageContext.request.contextPath}/WebRoot/plugin/bootstrap-3.3.6-dist/js/bootbox.min.js"></script>
<!-- import default style -->
<link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/WebRoot/plugin/bootstrap-3.3.6-dist/css/bootstrap.min.css">
